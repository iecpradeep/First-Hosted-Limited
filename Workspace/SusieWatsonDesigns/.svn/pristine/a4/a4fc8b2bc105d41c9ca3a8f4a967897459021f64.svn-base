    <li><a href="<%=getCurrentAttribute("item","custitem_bb1_zoomimage01","")%>" rel="zoom-id:zoom-image" rev="<%=getCurrentAttribute("item","custitem_bb1_extraimage01","#")%>"><img src="<%=getCurrentAttribute("item","custitem_bb1_extraimage01","#")%>" alt="<%=getCurrentAttribute("item","storedisplayname")%>" /></a></li>
    <li><a href="<%=getCurrentAttribute("item","custitem_bb1_zoomimage02","")%>" rel="zoom-id:zoom-image" rev="<%=getCurrentAttribute("item","custitem_bb1_extraimage02","#")%>"><img src="<%=getCurrentAttribute("item","custitem_bb1_extraimage02","#")%>" alt="<%=getCurrentAttribute("item","storedisplayname")%>" /></a></li>
    <li><a href="<%=getCurrentAttribute("item","custitem_bb1_zoomimage03","")%>" rel="zoom-id:zoom-image" rev="<%=getCurrentAttribute("item","custitem_bb1_extraimage03","#")%>"><img src="<%=getCurrentAttribute("item","custitem_bb1_extraimage03","#")%>" alt="<%=getCurrentAttribute("item","storedisplayname")%>" /></a></li>
    <li><a href="<%=getCurrentAttribute("item","custitem_bb1_zoomimage04","")%>" rel="zoom-id:zoom-image" rev="<%=getCurrentAttribute("item","custitem_bb1_extraimage04","#")%>"><img src="<%=getCurrentAttribute("item","custitem_bb1_extraimage04","#")%>" alt="<%=getCurrentAttribute("item","storedisplayname")%>" /></a></li>
   </ul>
   
  </div>
  
  <DISPLAY_OUTOFSTOCK_STATUS>
  
  <div class="text">
  
   <form method="post" name="form<%=getCurrentAttribute("item", "internalid")%>" id="form<%=getCurrentAttribute("item", "internalid")%>" onsubmit="if (bb1ValidateBuyQuantity(this, '<%=getCurrentAttribute("customer","pricelevel","")%>', '<%=getCurrentAttribute("customer","custentity_bb1_overridemintradeqty","F")%>')) return checkmandatory<%=getCurrentAttribute("item", "internalid")%>();" action="/app/site/backend/additemtocart.nl">
    
    <div class="product-details">
     
     <h1 id="product-details" itemprop="name"><%=getCurrentAttribute("item","storedisplayname")%></h1>
     
