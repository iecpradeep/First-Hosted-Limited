  <div class="line"></div>

  <div class="reviews"><%=getCurrentAttribute("item","custitem_bb1_feedback")%></div>
  
 </div>
 
 <script type="text/javascript">
 //<![CDATA[
  (function() {
   
   //$("#cart_<%=getCurrentAttribute("item","internalid")%> .item-options:has(input[type!='hidden'],select)").css({ display: "block" });
   
   if (<%=getCurrentAttribute("item","quantityavailable","0")%><=0 && "<%=getCurrentAttribute("item","outofstockbehavior")%>"=="Disallow back orders but display out-of-stock message") $("#cart_<%=getCurrentAttribute("item","internalid")%> .add-to-basket, #cart_<%=getCurrentAttribute("item","internalid")%> .quantity").css({ display: "none" });
   
   window.orgpricerange<%=getCurrentAttribute("item","internalid")%> = $('#itemprice<%=getCurrentAttribute("item","internalid")%>').html();

   try {
    var matrixdata = eval('<%=getCurrentAttribute("item","custitem_bb1_matrixdata","")%>');
   }
   catch(e) {}
   
   var UpdatePricing = function(price, wasprice) {
    
    var container = $("div.product-intro2");
    
    if (price > 0 && wasprice > 0 && price != wasprice) {
     if (!isNaN(price)) price = price.formatCurrency();
     container.find(".price").addClass("marked-down");
     container.find(".was-price").html("Was: " + wasprice.formatCurrency());
     container.find(".sale-price-label").html("Now: ");
    }
    else {
     if (!isNaN(price)) price = price.formatCurrency();
     container.find(".price").removeClass("marked-down");
     container.find(".was-price").html("");
     container.find("sale-price-label").html("");      
    }
    
   };
   
   if (matrixdata && matrixdata.length > 0) { // setup event handlers for matrix items
    
    $(".item-options select").each(function() { 
     this.ns_onchange = this.onchange;
     this.onchange = function(e) {
      if (typeof this.ns_onchange == "function")
       this.ns_onchange();
      // get selected options
      var options_l = {}, options_a = [];
      $(".item-options select option:selected").each(function() {
       var $this = $(this)
       var val = $this.val();
       if (val != "") {
        options_a.push(val);
        options_l[$this.parent().attr("id")] = val;
       }
      });
      
      if (options_a.length > 0 && typeof window.Item<%=getCurrentAttribute("item","internalid")%>_prices[options_a.join(":")] != "undefined") {
      
       var child_price = window.Item<%=getCurrentAttribute("item","internalid")%>_prices[options_a.join(":")].parseCurrency();
       var child_wasprice = "";
       
       for (var i=0, l=matrixdata.length; i < l; i++) {
       
        var match = true;
        for (opt in options_l) {
         if (matrixdata[i][opt] != options_l[opt]) {
          match = false;
          break;
         }
        }
        
        if (match) {
         child_wasprice = matrixdata[i]["wasprice"];
         break;
        }
        
       }
       
       UpdatePricing(child_price, child_wasprice);
       
      }
      else {

       UpdatePricing(0, 0);

      }
      
     };
    });
   }
   else { // display was pricing for non-matrix items
    
    var container = $("div.product-intro2");
    var ns_price = container.find(".sale-price");
   
    if (ns_price.find("table").length == 0) {
     var price = ns_price.text().parseCurrency();
     var wasprice = container.find("input[name=wasprice]").val().parseCurrency();
     UpdatePricing(price, wasprice);
    }
    
   }
   
   var $imagegallery = $("div.image-gallery");
  
   $imagegallery.each(function() {
   
    var $this = $(this);
    
    // Remove Sale status for trade
    
    var pricelevel = "<%=getCurrentAttribute("customer","pricelevel","")%>";
    if (/Trade/i.test(pricelevel))
     $this.find("div.image span.status:containsAny(Sale Special Offer Off special offer off)").remove();
    
   });
   
   $imagegallery.find("#zoom-image").hover(function() {
    $("#facebook_like_compact").css("visibility", "hidden");
    $('[data-pin-log="button_pinit"]').css("visibility", "hidden");
   }, function() {
    $("#facebook_like_compact").css("visibility", "");
    $('[data-pin-log="button_pinit"]').css("visibility", "");
   });
   
   $imagegallery.find("ul.img-list li").hide().not(":has(img[src$='#'])").show().find("a").each(function() {
    var resized_url = this.getAttribute("rev");
    var parms = {"resizeid":"-2", "resizeh":"350", "resizew":"350"};
    for (var key in parms)
     resized_url = addParamToURL(resized_url, key, parms[key], true);
    this.setAttribute("rev", resized_url);
   }).find("img").each(function() {
    var resized_url = this.src;
    var parms = {"resizeid":"-9", "resizeh":"67", "resizew":"67"};
    for (var key in parms)
     resized_url = addParamToURL(resized_url, key, parms[key], true);
    this.src = resized_url;
   });
   
   $("div.product-rating img[src$='blank.gif']").parent().hide();
   
   $("ul.links li:last").addClass("last");
   
   $("#reviews .reviews:empty").parent().hide();
   
   $("input#qty[type='hidden']").before('<input type="text" id="qty" value="1" disabled />');
   
  })();
 //]]>
 </script>
</td>