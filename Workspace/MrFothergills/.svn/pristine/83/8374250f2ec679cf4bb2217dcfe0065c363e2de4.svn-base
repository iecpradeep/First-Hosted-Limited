/* SlidersJS */

#slider{
	position: relative;
	/*height: 280px;*/
	margin: 10px 30px 15px;
}

.slidesjs-pagination{
	position: absolute;
	bottom: 0;
	right: 0;
	z-index: 10;
	background-color: #fff;
	list-style: none outside none;
	border-radius: 10px 0 0;
}

.slidesjs-pagination li{
	float: left;
	margin: 5px 0 5px 5px;
}

.slidesjs-pagination li a{
	height: 0;
	padding-top: 13px;
	overflow: hidden;
	width: 13px;
	display: block;
	background-image: url('/site/mrfothergills2/img/pagination.png');
}

.slidesjs-pagination li a.active, .slidesjs-pagination li a.active:hover{
	background-position: 0 -13px;
}

.slidesjs-pagination li a:hover{
	background-position: 0 -26px;
}

.slide{
	position: absolute;
	display: block;
	height: 100%;
	width: 100%;
}

.slide_text{
	float: right;
	margin: 10px;
	width: 273px;
}

.slide p{
	margin: 5px 0 5px;
	text-align: center;
}

.slide_large{
	color: #62b5cf;
	font-weight: bold;
	font-size: 22px;
