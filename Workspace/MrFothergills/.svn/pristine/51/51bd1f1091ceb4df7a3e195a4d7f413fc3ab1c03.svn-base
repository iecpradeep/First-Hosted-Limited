<script type="text/javascript" src="/c.3322617/site/common/js/WebStoreFinder.js"></script><script type="text/javascript">var pcaAccountCode="MRFOT11112"; var pcaLicenseKey="TM39-PP44-ET44-CG72"; pcaUseUKData = true;</script>
<script type="text/javascript">
$(document).ready(function(){
$('#country').val('GB');
$('#country').attr('disabled', 'disabled');
$('#custentity_mrf_cust_brand').val('1');
$('#salutation').val($('#title-select').val());
$('#custentity_catalogueall_fs_inp').prop('checked', true);
$('#manselection input[type="checkbox"]').prop('checked', false).prop('disabled', true);
$('#title-select').change(function(){
	$('#salutation').val($(this).val());
});
$('#custentity_catalogueall_fs_inp').change(function(){
	if($('#custentity_catalogueall_fs_inp').prop('checked'))
	{
		$('#manselection input[type="checkbox"]').prop('checked', false).prop('disabled', true);
	}
	else
	{
		$('#manselection input[type="checkbox"]').prop('checked', false).prop('disabled', false);
	}
});
$('#manselection input[type="checkbox"]').change(function(){
	if ($('#custentity_cataloguea_fs_inp').prop('checked') &&
		$('#custentity_catalogueb_fs_inp').prop('checked') &&
		$('#custentity_cataloguec_fs_inp').prop('checked') &&
		$('#custentity_catalogued_fs_inp').prop('checked'))
	{
		$('#manselection input[type="checkbox"]').prop('checked', false).prop('disabled', true);
		$('#custentity_catalogueall_fs_inp').prop('checked', true);
	}
});
});
</script>
</head>
